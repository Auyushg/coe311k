    "import math\n",
    "\n",
    "def convertToBinary(num):\n",
    "    binary = \"\"\n",
    "    while num > 0:\n",
    "        if(num % 2 == 0):\n",
    "            binary = \"0\" + binary\n",
    "        else:\n",
    "            binary = \"1\" + binary\n",
    "        num = num // 2\n",
    "        \n",
    "    return binary\n",
    "        "
   ]
